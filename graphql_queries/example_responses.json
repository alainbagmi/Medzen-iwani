  "common_queries": {
    "find_doctors_in_area": {
      "description": "Find all medical doctors in a specific country/region",
      "graphql": "query FindDoctorsInArea($country: String!) { medical_provider_profilesCollection(filter: { professional_role: { eq: \"Medical Doctor\" }, users: { country: { eq: $country } } }) { edges { node { id professional_role users { full_name country } } } } }"
    },

    "top_rated_providers": {
      "description": "Get top-rated providers by patient satisfaction",
      "graphql": "query TopRatedProviders($limit: Int!) { medical_provider_profilesCollection(first: $limit, orderBy: { patient_satisfaction_avg: DescNullsLast }, filter: { application_status: { eq: \"approved\" } }) { edges { node { id professional_role patient_satisfaction_avg total_consultations users { full_name } } } } }"
    },

    "telemedicine_enabled_providers": {
      "description": "Find providers with telemedicine capabilities",
      "graphql": "query TelemedicineProviders { medical_provider_profilesCollection(filter: { video_consultation_enabled: { eq: true }, application_status: { eq: \"approved\" } }) { edges { node { id professional_role video_consultation_enabled audio_consultation_enabled chat_consultation_enabled users { full_name phone_number } } } } }"
    },

    "providers_by_language": {
      "description": "Find providers who speak specific languages",
      "graphql": "query ProvidersByLanguage($language: String!) { medical_provider_profilesCollection(filter: { languages_spoken: { contains: [$language] } }) { edges { node { id professional_role languages_spoken users { full_name } } } } }"
    }
  }
