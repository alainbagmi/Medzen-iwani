                ["id0.17"] = <
                    text = <"Signes vitaux">
                    description = <"Pression artérielle, fréquence cardiaque, fréquence respiratoire, saturation en oxygène (particulièrement pour les patients cardiopulmonaires).">
                >
                ["id0.18"] = <
                    text = <"Évaluation objective">
                    description = <"Résultats de l'examen physique, inspection, palpation, observations cliniques.">
                >
                ["id0.19"] = <
                    text = <"Évaluation de la posture et de l'alignement">
                    description = <"Posture statique et dynamique, alignement vertébral, alignement pelvien, position des pieds.">
                >
                ["id0.20"] = <
                    text = <"Évaluation de l'amplitude de mouvement (ADM)">
                    description = <"ADM active et passive des articulations affectées, mesures goniométriques, évaluation de la fin de mouvement.">
                >
                ["id0.21"] = <
                    text = <"Test de la force musculaire (TMM)">
                    description = <"Test musculaire manuel utilisant l'échelle de notation 0-5, groupes musculaires spécifiques testés.">
                >
                ["id0.22"] = <
                    text = <"Test de flexibilité et de longueur musculaire">
                    description = <"Raideur musculaire, flexibilité articulaire, tests spéciaux (test de Thomas, élévation de la jambe droite, etc.).">
                >
                ["id0.23"] = <
                    text = <"Analyse de la marche">
                    description = <"Patron de marche, utilisation d'aide technique, cadence, longueur du pas, déviations, montée d'escaliers.">
                >
                ["id0.24"] = <
                    text = <"Évaluation de l'équilibre et de la coordination">
                    description = <"Équilibre statique et dynamique, échelle d'équilibre de Berg, test chronométré de lever et marcher (TUG), appui unipodal, tests de coordination.">
                >
                ["id0.25"] = <
                    text = <"Évaluation de la mobilité fonctionnelle">
                    description = <"Mobilité au lit, transferts (assis-debout, lit-chaise), portée fonctionnelle, activités de la vie quotidienne (AVQ).">
                >
                ["id0.26"] = <
                    text = <"Évaluation neurologique">
                    description = <"Sensation (toucher léger, proprioception), réflexes (ROT), tonus (spasticité, rigidité), coordination.">
                >
                ["id0.27"] = <
                    text = <"Évaluation cardiopulmonaire">
                    description = <"Tolérance à l'exercice, échelle de dyspnée, test de marche de 6 minutes, réponse de la fréquence cardiaque à l'activité.">
                >
                ["id0.28"] = <
                    text = <"Tests spéciaux">
                    description = <"Tests spécifiques à la condition (Lachman, McMurray, Neer, Hawkins, Phalen, Tinel, etc.).">
                >
                ["id0.29"] = <
                    text = <"Mesures de résultats et échelles fonctionnelles">
                    description = <"Échelles standardisées telles que FIM, indice de Barthel, LEFS, DASH, Oswestry, NDI, SF-36.">
                >
