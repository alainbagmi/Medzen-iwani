description
    original_author = <
        ["date"] = <"2025-02-11">
        ["name"] = <"MedZen Health">
        ["organisation"] = <"MedZen">
        ["email"] = <"info@medzenhealth.app">
    >
    lifecycle_state = <"published">
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record a comprehensive physiotherapy session including assessment, treatment interventions, rehabilitation progress, and functional outcomes.">
            use = <"Use for documenting physiotherapy sessions including:
- Musculoskeletal physiotherapy (orthopedic, sports injuries)
- Neurological physiotherapy (stroke, spinal cord injury, TBI)
- Cardiopulmonary physiotherapy (post-cardiac surgery, COPD)
- Pediatric physiotherapy (developmental delays, cerebral palsy)
- Geriatric physiotherapy (falls prevention, mobility)
- Post-surgical rehabilitation
- Chronic pain management
- Range of motion and strength assessment
- Gait and balance training
- Functional mobility assessment
- Home exercise program prescription">
            keywords = <"physiotherapy", "physical therapy", "rehabilitation", "range of motion", "strength", "gait", "balance", "mobility", "exercise", "manual therapy", "electrotherapy", "functional assessment">
            copyright = <"© MedZen Health 2025">
        >
        ["fr"] = <
            language = <[ISO_639-1::fr]>
            purpose = <"Pour enregistrer une séance de physiothérapie complète comprenant l'évaluation, les interventions de traitement, les progrès de la réadaptation et les résultats fonctionnels.">
            use = <"Utiliser pour documenter les séances de physiothérapie incluant:
- Physiothérapie musculo-squelettique (orthopédique, blessures sportives)
- Physiothérapie neurologique (AVC, lésion médullaire, TCC)
- Physiothérapie cardiopulmonaire (post-chirurgie cardiaque, MPOC)
- Physiothérapie pédiatrique (retards de développement, paralysie cérébrale)
- Physiothérapie gériatrique (prévention des chutes, mobilité)
- Réadaptation post-chirurgicale
- Gestion de la douleur chronique
- Évaluation de l'amplitude de mouvement et de la force
- Entraînement de la marche et de l'équilibre
- Évaluation de la mobilité fonctionnelle
- Prescription de programme d'exercices à domicile">
            keywords = <"physiothérapie", "kinésithérapie", "réadaptation", "amplitude de mouvement", "force", "marche", "équilibre", "mobilité", "exercice", "thérapie manuelle", "électrothérapie", "évaluation fonctionnelle">
            copyright = <"© MedZen Health 2025">
        >
    >

