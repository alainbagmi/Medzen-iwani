description
    original_author = <
        ["date"] = <"2025-11-02">
        ["name"] = <"MedZen Development Team">
        ["organisation"] = <"MedZen Health">
        ["email"] = <"info@medzenhealth.app">
    >
    lifecycle_state = <"unmanaged">
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record a comprehensive psychiatric assessment encounter including mental health evaluation, psychiatric diagnoses, mental status examination, suicide risk assessment, psychiatric medication management, and psychotherapy documentation for the MedZen health records system.">
            use = <"Use to document psychiatric consultations covering mental health conditions including depression, anxiety disorders, psychotic disorders, bipolar disorder, substance use disorders, personality disorders, and other psychiatric conditions. Supports diagnostic assessment, risk evaluation, treatment planning, and psychiatric medication management.">
            keywords = <"psychiatry", "mental health", "depression", "anxiety", "psychosis", "bipolar", "suicide risk", "mental status", "psychotherapy", "psychiatric medication", "schizophrenia", "PTSD", "OCD", "substance abuse">
        >
        ["fr"] = <
            language = <[ISO_639-1::fr]>
            purpose = <"Enregistrer une évaluation psychiatrique complète comprenant l'évaluation de la santé mentale, les diagnostics psychiatriques, l'examen de l'état mental, l'évaluation du risque suicidaire, la gestion des médicaments psychiatriques et la documentation de la psychothérapie pour le système de dossiers de santé MedZen.">
            use = <"Utiliser pour documenter les consultations psychiatriques couvrant les affections de santé mentale dont la dépression, les troubles anxieux, les troubles psychotiques, le trouble bipolaire, les troubles liés à l'utilisation de substances, les troubles de la personnalité et autres affections psychiatriques. Prend en charge l'évaluation diagnostique, l'évaluation des risques, la planification du traitement et la gestion des médicaments psychiatriques.">
            keywords = <"psychiatrie", "santé mentale", "dépression", "anxiété", "psychose", "bipolaire", "risque suicidaire", "état mental", "psychothérapie", "médicament psychiatrique", "schizophrénie", "TSPT", "TOC", "toxicomanie">
        >
    >
    other_details = <
        ["licence"] = <"This work is licensed under the Creative Commons Attribution-ShareAlike 4.0 International License.">
        ["custodian_organisation"] = <"MedZen Health">
        ["references"] = <"DSM-5-TR, ICD-11, Columbia-Suicide Severity Rating Scale (C-SSRS), PHQ-9, GAD-7, BPRS, PANSS">
    >

definition
    COMPOSITION[id1.1] matches {    -- Psychiatric Assessment
        category matches {
