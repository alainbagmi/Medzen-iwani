                                value matches {
                                    DV_CODED_TEXT[id0.4] matches {
                                        defining_code matches {
                                            [local::
                                            at0.9,     -- Major depressive disorder
                                            at0.10,    -- Generalized anxiety disorder
                                            at0.11,    -- Panic disorder
                                            at0.12,    -- Social anxiety disorder
                                            at0.13,    -- Obsessive-compulsive disorder
                                            at0.14,    -- Post-traumatic stress disorder
                                            at0.15,    -- Bipolar I disorder
                                            at0.16,    -- Bipolar II disorder
                                            at0.17,    -- Schizophrenia
                                            at0.18,    -- Schizoaffective disorder
                                            at0.19,    -- Substance use disorder
                                            at0.20,    -- Personality disorder
                                            at0.21,    -- Eating disorder
                                            at0.22,    -- Adjustment disorder
                                            at0.23,    -- Attention-deficit/hyperactivity disorder
                                            at0.24]    -- Other psychiatric condition
                                        }
                                    }
                                }
                            }
                            ELEMENT[id0.5] occurrences matches {0..1} matches {    -- Suicide Risk Level
                                value matches {
                                    DV_CODED_TEXT[id0.6] matches {
                                        defining_code matches {
                                            [local::
                                            at0.25,    -- No risk
                                            at0.26,    -- Low risk
                                            at0.27,    -- Moderate risk
                                            at0.28,    -- High risk
                                            at0.29]    -- Imminent risk
                                        }
                                    }
                                }
                            }
                            ELEMENT[id0.7] occurrences matches {0..1} matches {    -- Violence Risk Level
                                value matches {
