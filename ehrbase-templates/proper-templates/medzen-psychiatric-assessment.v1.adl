                                            at0.3,     -- Medication management
                                            at0.4,     -- Crisis intervention
                                            at0.5,     -- Pre-admission assessment
                                            at0.6,     -- Discharge planning
                                            at0.7,     -- Psychotherapy session
                                            at0.8]     -- Court-ordered evaluation
                                        }
                                    }
                                }
