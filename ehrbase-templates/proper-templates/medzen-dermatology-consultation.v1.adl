                                        defining_code matches {
                                            [local::
                                            at0.19,    -- Atopic dermatitis / Eczema
                                            at0.20,    -- Psoriasis
                                            at0.21,    -- Acne vulgaris
                                            at0.22,    -- Acne rosacea
                                            at0.23,    -- Contact dermatitis
                                            at0.24,    -- Seborrheic dermatitis
                                            at0.25,    -- Urticaria (hives)
                                            at0.26,    -- Fungal infection (dermatophytosis)
                                            at0.27,    -- Bacterial skin infection
                                            at0.28,    -- Viral skin infection
                                            at0.29,    -- Basal cell carcinoma
                                            at0.30,    -- Squamous cell carcinoma
                                            at0.31,    -- Melanoma
                                            at0.32,    -- Vitiligo
                                            at0.33,    -- Alopecia
                                            at0.34,    -- Hidradenitis suppurativa
                                            at0.35,    -- Skin manifestation of systemic disease
                                            at0.36,    -- Drug eruption
                                            at0.37,    -- Other dermatological condition
                                            at0.38]    -- No specific diagnosis yet
                                        }
                                    }
                                }
                            }
                            ELEMENT[id0.7] occurrences matches {0..1} matches {    -- Phototype (Fitzpatrick Scale)
                                value matches {
                                    DV_CODED_TEXT[id0.8] matches {
                                        defining_code matches {
                                            [local::
                                            at0.39,    -- Type I (Very fair, always burns)
                                            at0.40,    -- Type II (Fair, usually burns)
                                            at0.41,    -- Type III (Medium, sometimes burns)
                                            at0.42,    -- Type IV (Olive, rarely burns)
                                            at0.43,    -- Type V (Brown, very rarely burns)
                                            at0.44]    -- Type VI (Black, never burns)
                                        }
                                    }
                                }
                            }
                            ELEMENT[id0.9] occurrences matches {0..1} matches {    -- Tele-dermatology Consultation
                                value matches {
                                    DV_BOOLEAN[id0.10] matches {
                                        value matches {True, False}
                                    }
                                }
                            }
                            allow_archetype CLUSTER[id0.11] occurrences matches {0..*} matches {    -- Encounter Identifiers
                                include
                                    archetype_id/value matches {/openEHR-EHR-CLUSTER\.identifier\.v0/}
                            }
                        }
                    }
                }
            }
        }
