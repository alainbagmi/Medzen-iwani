description
    original_author = <
        ["date"] = <"2025-02-11">
        ["name"] = <"MedZen Health">
        ["organisation"] = <"MedZen">
        ["email"] = <"info@medzenhealth.app">
    >
    lifecycle_state = <"published">
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record a comprehensive dermatology consultation including skin examination, lesion documentation, dermatoscopy findings, and treatment planning for various dermatological conditions.">
            use = <"Use for documenting dermatology consultations covering:
- Skin lesion documentation and morphology
- Eczema, psoriasis, acne, dermatitis
- Skin infections (bacterial, viral, fungal)
- Skin cancer screening and diagnosis
- Tele-dermatology with image capture
- Dermatological procedures (biopsy, excision, cryotherapy, laser treatment)
- Phototherapy documentation
- Allergy and patch testing
- Cosmetic dermatology
- Pediatric dermatology">
            keywords = <"dermatology", "skin", "lesion", "rash", "eczema", "psoriasis", "acne", "melanoma", "dermatoscopy", "biopsy", "cryotherapy", "laser", "phototherapy", "tele-dermatology">
            copyright = <"© MedZen Health 2025">
        >
        ["fr"] = <
            language = <[ISO_639-1::fr]>
            purpose = <"Pour enregistrer une consultation dermatologique complète comprenant l'examen cutané, la documentation des lésions, les résultats de dermatoscopie et la planification du traitement pour diverses affections dermatologiques.">
            use = <"Utiliser pour documenter les consultations dermatologiques couvrant:
- Documentation des lésions cutanées et morphologie
- Eczéma, psoriasis, acné, dermatite
- Infections cutanées (bactériennes, virales, fongiques)
- Dépistage et diagnostic du cancer de la peau
- Télé-dermatologie avec capture d'images
- Procédures dermatologiques (biopsie, excision, cryothérapie, traitement au laser)
- Documentation de la photothérapie
- Tests d'allergie et patch tests
- Dermatologie esthétique
- Dermatologie pédiatrique">
            keywords = <"dermatologie", "peau", "lésion", "éruption", "eczéma", "psoriasis", "acné", "mélanome", "dermatoscopie", "biopsie", "cryothérapie", "laser", "photothérapie", "télé-dermatologie">
            copyright = <"© MedZen Health 2025">
        >
    >

definition
    COMPOSITION[id1.1] matches {    -- Dermatology Consultation
