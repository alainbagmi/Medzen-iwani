description
    original_author = <
        ["date"] = <"2025-11-02">
        ["name"] = <"MedZen Development Team">
        ["organisation"] = <"MedZen Health">
        ["email"] = <"info@medzenhealth.app">
    >
    lifecycle_state = <"unmanaged">
    details = <
        ["en"] = <
            language = <[ISO_639-1::en]>
            purpose = <"To record a comprehensive pulmonology consultation encounter including asthma management, COPD assessment, pulmonary function tests, sleep apnea evaluation, lung infections, oxygen therapy, and smoking cessation counseling for the MedZen health records system.">
            use = <"Use to document pulmonology consultations covering respiratory conditions including asthma, COPD, sleep disorders, pulmonary infections, and respiratory rehabilitation. Supports pulmonary function testing, imaging interpretation, and long-term respiratory disease management.">
            keywords = <"pulmonology", "respiratory", "asthma", "COPD", "spirometry", "lung function", "sleep apnea", "pneumonia", "oxygen therapy", "smoking cessation", "inhaler", "breathlessness", "dyspnea">
        >
        ["fr"] = <
            language = <[ISO_639-1::fr]>
            purpose = <"Enregistrer une consultation complète de pneumologie comprenant la gestion de l'asthme, l'évaluation de la BPCO, les tests de fonction pulmonaire, l'évaluation de l'apnée du sommeil, les infections pulmonaires, l'oxygénothérapie et le conseil en sevrage tabagique pour le système de dossiers de santé MedZen.">
            use = <"Utiliser pour documenter les consultations de pneumologie couvrant les affections respiratoires dont l'asthme, la BPCO, les troubles du sommeil, les infections pulmonaires et la réhabilitation respiratoire. Prend en charge les tests de fonction pulmonaire, l'interprétation d'imagerie et la gestion des maladies respiratoires à long terme.">
            keywords = <"pneumologie", "respiratoire", "asthme", "BPCO", "spirométrie", "fonction pulmonaire", "apnée du sommeil", "pneumonie", "oxygénothérapie", "sevrage tabagique", "inhalateur", "essoufflement", "dyspnée">
        >
    >
    other_details = <
        ["licence"] = <"This work is licensed under the Creative Commons Attribution-ShareAlike 4.0 International License.">
        ["custodian_organisation"] = <"MedZen Health">
        ["references"] = <"Global Initiative for Asthma (GINA) 2024 Guidelines, Global Initiative for Chronic Obstructive Lung Disease (GOLD) 2024 Report, American Thoracic Society/European Respiratory Society Standards">
    >

definition
