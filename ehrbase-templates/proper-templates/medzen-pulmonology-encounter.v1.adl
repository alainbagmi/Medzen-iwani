                                        defining_code matches {
                                            [local::
                                            at0.29,    -- Current smoker
                                            at0.30,    -- Former smoker
                                            at0.31,    -- Never smoked
                                            at0.32]    -- Passive smoking exposure
                                        }
                                    }
                                }
                            }
                            ELEMENT[id0.11] occurrences matches {0..1} matches {    -- Pack Years
                                value matches {
                                    DV_QUANTITY[id0.12] matches {
                                        units matches {"pack-years"}
                                        magnitude matches {|>=0.0|}
                                    }
                                }
                            }
                        }
                    }
                }
            }
        }
